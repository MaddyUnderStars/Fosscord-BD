{
	"dependencies": {
		"@discordjs/collection": "^0.7.0",
		"@ittai/builder": "^2.0.5",
		"ittai": "^2.0.7",
		"patch-package": "^6.4.7"
	},
	"devDependencies": {
		"@types/react": "^18.0.14"
	},
	"name": "fosscord-bd",
	"version": "1.0.0",
	"main": "src/index.ts",
	"scripts": {
		"postinstall": "patch-package",
		"build": "ittai --powercordv2=\"./build\"",
		"release": "ittai --betterdiscord=\"./build\" --production",
		"build:bd": "ittai --betterdiscord=\"%appdata%/BetterDiscord/plugins\"",
		"release:bd": "ittai --betterdiscord=\"%appdata%/BetterDiscord/plugins\" --production"
	},
	"author": "MaddyUnderStars",
	"license": "AGPLV3",
	"description": "Discord client plugin allowing connections to Fosscord Instances"
}
